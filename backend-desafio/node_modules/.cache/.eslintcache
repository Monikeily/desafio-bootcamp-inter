{\n                ...state,\n                value: (action.cart.value - action.product.quantity),\n                Cart: state.Cart.filter(item => {\n                    return item.id !== action.product.id\n                })\n            }\n        case 'CHANGE_CART':\n            return state = action.localCart\n        default:\n            return state;\n    }\n    return state\n}\n","C:\\Users\\Monikeily\\Desktop\\65e92cc9-eb62-482f-9656-d073f37a74c9\\dioshopping-main\\src\\components\\store\\reducers\\products.js",[],"C:\\Users\\Monikeily\\Desktop\\65e92cc9-eb62-482f-9656-d073f37a74c9\\dioshopping-main\\src\\components\\store\\reducers\\product.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":41,"column":9,"nodeType":"79","messageId":"80","endLine":44,"endColumn":10},{"ruleId":"81","severity":1,"message":"82","line":14,"column":15,"nodeType":"83","endLine":18,"endColumn":6},{"ruleId":"84","severity":1,"message":"85","line":40,"column":1,"nodeType":"86","endLine":46,"endColumn":2},{"ruleId":"87","severity":1,"message":"88","line":23,"column":44,"nodeType":"83","messageId":"89","endLine":23,"endColumn":46},{"ruleId":"90","severity":1,"message":"91","line":73,"column":5,"nodeType":"92","messageId":"93","endLine":73,"endColumn":17},"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"no-lone-blocks","Nested block is redundant.","BlockStatement","redundantNestedBlock","react-hooks/exhaustive-deps","Effect callbacks are synchronous to prevent race conditions. Put the async function inside:\n\nuseEffect(() => {\n  async function fetchData() {\n    // You can await here\n    const response = await MyAPI.getData(someId);\n    // ...\n  }\n  fetchData();\n}, [someId]); // Or [] if effect doesn't need props or state\n\nLearn more about data fetching with Hooks: https://reactjs.org/link/hooks-data-fetching","ArrowFunctionExpression","import/no-anonymous-default-export","Assign object to a va